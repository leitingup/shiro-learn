<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.shiro.demo01.mapper.UserMapper">
    <select id="queryUserByName" parameterType="String" resultType="org.shiro.demo01.pojo.User">
        select * from shiro_mybaits.user where name=#{name};
    </select>
</mapper>